class Stack:
    def __init__(self):
        self.items = []

    def push(self, item):
        self.items.append(item)

    def pop(self):
        if not self.is_empty():
            return self.items.pop()
        else:
            return None  # или можно поднять исключение

    def peek(self):
        if not self.is_empty():
            return self.items[-1]
        else:
            return None

    def is_empty(self):
        return len(self.items) == 0

    def size(self):
        return len(self.items)

# Пример использования
stack = Stack()
stack.push(10)
stack.push(20)
stack.push(30)

print("Текущий стек:", stack.items)

print("Выталкиваем элемент:", stack.pop())
print("После выталкивания:", stack.items)

print("Верхний элемент (peek):", stack.peek())

print("Стек пустой?", stack.is_empty())

print("Размер стека:", stack.size())
